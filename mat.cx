package mat

//-----------------------------------------------------------------------------
//-----------------------------------------------------------------------------
type v4 struct {
    x f32
    y f32
    z f32
    w f32
}

//-----------------------------------------------------------------------------
//-----------------------------------------------------------------------------
func v4_(x f32, y f32, z f32, w f32) (out v4) {
    out.x = x
    out.y = y
    out.z = z
    out.w = w
}

